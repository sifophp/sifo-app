{
  "name": "sifo/sifo-app",
  "description": "The default sifo application using the SIFO framework",
  "license": "Apache-2.0",
  "keywords": [],
  "homepage": "https://github.com/sifophp/sifo-app",
  "type": "project",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "sifophp/sifo": "dev-sifo3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Example\\": "instances/example/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Example\\": "instances/example/test/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
